<% include header %>
<div class="container">
<form class="form-horizontal" id="userForm">
  <div class="form-group">
    <label for="telephone" class="col-md-1 col-xs-2  control-label">账号:</label>
    <div class="col-md-11 col-xs-10">
      <input type="text" class="form-control " name="telephone" id="telephone" placeholder="手机号">
    </div>
  </div>
  <div class="form-group">
    <label for="password" class="col-md-1 col-xs-2 control-label">密码:</label>
    <div class="col-md-11 col-xs-10">
      <input type="password" class="form-control " name="password" id="password" placeholder="密码">
    </div>
  </div>
  <div class="form-group">
    <div class="col-md-offset-1 col-md-11 col-xs-offset-2 col-xs-10">
      <button type="submit" id="login" class="btn btn-default">登陆</button>
    </div>
  </div>
  <div class="alert alert-success hide" id="tip" role="alert"></div>
</form>
</div>
  <% include footer %>